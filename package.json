{
	"name": "shoot-or-not",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev && npx prisma generate",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"generate-prisma-client": "dotenv -e .env.production -- npx prisma generate",
		"migrate": "dotenv -e .env -- npx prisma migrate dev",
		"migrate:prod": "dotenv -e .env.production -- npx prisma migrate deploy",
		"update-database": "dotenv -e .env -- npx prisma db push",
		"update-database:prod": "dotenv -e .env.production -- npx prisma db push",
		"downmigration": "npx prisma migrate diff --from-schema-datamodel prisma/schema.prisma --to-schema-datasource prisma/schema.prisma --script > ./prisma/down.sql",
		"seed": "npx prisma db seed"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "^2.10.0",
		"@skeletonlabs/tw-plugin": "^0.4.0",
		"@sveltejs/adapter-node": "^5.2.0",
		"@sveltejs/adapter-vercel": "^5.3.0",
		"@sveltejs/kit": "^2.5.18",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.10",
		"@types/eslint": "^8.56.0",
		"@types/node": "^20.12.12",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.33",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^4.2.7",
		"svelte-check": "^3.8.5",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@fortawesome/fontawesome-free": "^6.5.2",
		"@prisma/client": "^5.14.0",
		"chart.js": "^4.4.3",
		"dotenv": "^16.4.5",
		"prisma": "^5.14.0"
	}
}
